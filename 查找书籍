#include<stdio.h>
#include<stdlib.h>
struct book
{
	char name[100];
	double price;
}shu[100];
int main()
{
	int n,i,max,min,t1,t2;
	scanf("%d\n",&n);
	for(i=0;i<n;i++)
	{
		if(i<n-1)
		{
		gets(shu[i].name);
		scanf("%lf\n",&shu[i].price);
		}else{
			gets(shu[i].name);
		scanf("%lf",&shu[i].price);
		}
	}
	max=min=shu[0].price;
	for(i=0;i<n;i++)
	{
		if(max<shu[i].price)
		{
			t1=i;
		}else{
			t2=i;
		}
	}
	printf("%.2lf, %s\n",shu[t1].price,shu[t1].name);
	printf("%.2lf, %s\n",shu[t2].price,shu[t2].name);
}
