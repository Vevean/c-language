#include <stdio.h>
#define MAXS 15

void StringCount( char *s );
void ReadString( char *s ); /* 由裁判实现，略去不表 */

int main()
{
    char s[MAXS];

    ReadString(s);
    StringCount(s);

    return 0;
}
#include<string.h>
void StringCount( char *s )
{

int big=0,small=0,kong=0,num=0,other=0;
int i,n;
n=strlen(s);
for(i=0;i<n;i++)
{
	if(s[i]>='A'&&s[i]<='Z')
		big++;
	else if(s[i]>='a'&&s[i]<='z')
		small++;
	else if(s[i]==' ')
		kong++;
	else if(s[i]>='0'&&s[i]<='9')
		num++;
	else
		other++;
}
printf("%d %d %d %d %d",big,small,kong,num,other);
}
