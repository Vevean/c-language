#include <stdio.h>
#include <stdlib.h>
#include <string.h>

struct stud_node {
     int    num;
     char   name[20];
     int    score;
     struct stud_node *next;
};
struct stud_node *head, *tail;

void input();

int main()
{
    struct stud_node *p;
	
    head = tail = NULL;
    input();
    for ( p = head; p != NULL; p = p->next )
        printf("%d %s %d\n", p->num, p->name, p->score);

    return 0;
}
void input()
{
	int num;
	while(scanf("%d",&num)&&num!=0)
	{
	struct stud_node *p=(struct stud_node*)malloc(sizeof(struct stud_node));
	p->num=num;
	scanf(" %s %d\n",&p->name, &p->score);
	if(head==NULL)
	{
		head=p;
		tail=p;
	}else{
		tail->next=p;
		tail=p;
		p->next=NULL;
	}
	}
}
