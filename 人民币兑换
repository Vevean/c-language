#include<stdio.h>
int main()
{
	int x,y,z;
	int n,count=0;
	scanf("%d",&n);
	for(x=1;x<30;x++)
	{
		for(y=1;y<75;y++)
		{
			for(z=1;z<150;z++)
			{
				if(x+y+z==100&&5*x+2*y+z==150)
				{
					count++;
					if(count<=n)
					printf("%d %d %d\n",x,y,z);
				}
			}
		}
	}
 } 

#include<stdio.h>
int main()
{
	int n;
	scanf("%d",&n);
	int x,y,z,count=0;
	for(x=1;x<30;x++)
	{
		z=3*x+50;
		y=50-4*x;
		if(z>1&&y>1)
		{
			count++;
			if(count<=n)
			printf("%d %d %d\n",x,y,z);
		}
	}
}
